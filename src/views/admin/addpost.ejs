<%- include('static/head.ejs', { bot,user,path}) %>
<div id="content" class="main-content">
    <br><br><br><br><br><br>
    <div class="container">


    <form method="POST">
     

        <div class="form-row mb-4">
            <div class="form-group col-md-4">
                <label for="inputEmail4">Post Name<span style="color:red">*</span></label>
                <input type="text" class="form-control" id="postname" name="postname" placeholder="" required>
            </div>
            <div class="form-group col-md-4">
                <label for="inputEmail4">Post Description<span style="color:red">*</span></label>
                <input type="text" class="form-control" id="postdesc" name="postdesc" maxlength="100" placeholder="" required>
            </div>
            <div class="form-group col-md-4">
                <label for="inputState">Post Category<span style="color:red">*</span></label>
                <select id="category" name="category" class="form-control" required>
                    <option value="Site Updates">Javascript</option>
                    <option value="Discord Updates">Python</option>
                    <option value="News">HTML</option>
                </select>
            </div>
         </div>
        <div class="form-group mb-4">
            <label for="inputAddress">Main (Optional)</label>
            <textarea type="text" class="form-control" id="main" name="main"></textarea>
        </div>
         <div class="form-group">
            <label for="inputEmail4">Secondary (Optional)</label>
            <textarea type="text" class="form-control" id="commands" name="commands"></textarea>
        </div>
      <button type="submit" class="btn btn-primary mt-3"><i class="fa fa-save"></i> Submit</button>
    </form>
</div>
</div></div>
<%- include('static/foot.ejs', { bot,user,path}) %>